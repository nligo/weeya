<!DOCTYPE html>
<html lang="zh_CN">

<head>
    {% block meta %}
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="icon" href="{{ asset('favicon.ico') }}" type="image/x-icon"/>
    {% endblock %}


    <title>Weeya Baby! |{% block title %}{% endblock %}</title>
    {% block stylesheets %}
        <link href="{{ asset('assets/admin') }}/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
        <link href="{{ asset('assets/admin') }}/font-awesome/css/font-awesome.min.css" rel="stylesheet">
        <link href="{{ asset('assets/admin/build') }}/css/custom.min.css" rel="stylesheet">
    {% endblock %}

    {% block stylesheets_inline %}{% endblock %}
</head>
<body class="nav-md">
{% block body %}
    <div class="container body">
        <div class="main_container">
            {% block sidebar %}
                {% include "admin/common/sidebar.html.twig" %}
            {% endblock %}

            {% block top_nav %}
                {% include "admin/common/top_nav.html.twig" %}
            {% endblock %}

            {% block page_content %}

            {% endblock %}


            {% block footer %}
                {% include "admin/common/footer.html.twig" %}
            {% endblock %}

        </div>
    </div>
{% endblock %}

{% block javascripts %}
    <!-- jQuery -->
    <script src="{{ asset('assets/admin') }}/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="{{ asset('assets/admin') }}/bootstrap/dist/js/bootstrap.min.js"></script>

    <!-- bootstrap-progressbar -->
    <script src="{{ asset('assets/admin') }}/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>

    <script src="{{ asset('assets/admin/build') }}/js/custom.min.js"></script>
{% endblock %}

{% block javascripts_inline %}{% endblock %}
</body>
</html>
